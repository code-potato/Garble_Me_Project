<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.codepotato.view.EffectsConfigScr">

    <TextView
        android:text="EffectsConfig!"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:id="@+id/textView"/>
    <SeekBar
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:id="@+id/seekBar"
            android:layout_marginBottom="46dp"
            android:layout_above="@+id/seekBar2" android:layout_alignStart="@+id/seekBar2"/>
    <SeekBar
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:id="@+id/seekBar2"
            android:layout_alignParentBottom="true" android:layout_toRightOf="@+id/textView"
            android:layout_marginLeft="67dp" android:layout_marginBottom="203dp"/>
    <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView" android:layout_above="@+id/seekBar2" android:layout_alignParentStart="true"/>
    <SeekBar
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:id="@+id/seekBar3" android:layout_above="@+id/seekBar" android:layout_alignStart="@+id/seekBar"
            android:layout_marginBottom="31dp"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/editText"
            android:layout_alignBottom="@+id/seekBar3" android:layout_alignEnd="@+id/textView" android:text="Dry"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/editText2" android:layout_below="@+id/seekBar3" android:layout_alignEnd="@+id/editText"
            android:text="Wet"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/editText3"
            android:text="FeedBack" android:layout_alignBottom="@+id/seekBar2"
            android:layout_toRightOf="@+id/imageView"/>
    <SeekBar
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:id="@+id/seekBar4"
            android:layout_marginTop="31dp"
            android:layout_below="@+id/editText3" android:layout_alignParentEnd="true"
            android:layout_alignStart="@+id/seekBar2"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/editText4"
            android:text="Delay(ms)" android:layout_alignBottom="@+id/seekBar4"
            android:layout_toRightOf="@+id/imageView"/>

</RelativeLayout>
